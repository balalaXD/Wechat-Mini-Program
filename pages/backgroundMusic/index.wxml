<!--pages/backgroundMusic/index.wxml-->

<view class="page">
  <view class="page__hd">
    <text class="page__title">Background Music API</text>
  </view>
  <view class="page__bd">
    <view class="btn-area">
      <button type="primary" bindtap="play">Play</button>
      <button type="primary" bindtap="pause">Pause</button>
      <button type="primary" bindtap="seek">Seek</button>
      <button type="primary" bindtap="stop">Stop</button>
      <button type="primary" bindtap="getPlayState">GetPlayState
      </button>
    </view>
  </view>
</view>
